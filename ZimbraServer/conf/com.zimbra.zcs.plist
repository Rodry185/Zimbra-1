<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>com.zimbra.zcs</string>
	<key>Program</key>
	<string>/opt/zimbra/libexec/zcs</string>
	<key>ProgramArguments</key>
	<array>
		<string>zcs</string>
		<string>start</string>
	</array>
	<key>StandardOutPath</key>
	<string>/opt/zimbra/log/zcs.log</string>
	<key>OnDemand</key>
	<true/>
	<key>RunAtLoad</key>
	<true/>
	<key>UserName</key>
	<string>zimbra</string>
	<key>GroupName</key>
	<string>zimbra</string>
  <key>EnvironmentVariables</key>
  <dict>
    <key>PERL5LIB</key>
    <string>/opt/zimbra/zimbramon/lib/darwin-thread-multi-2level:/opt/zimbra/zimbramon/lib</string>
  </dict>
</dict>
</plist>
